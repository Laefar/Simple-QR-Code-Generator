{
    "manifest_version": 3,
    "name": "QR Code Generator",
    "description": "Generate a quick QR code for your links",
    "version": "1.0",
    "icons": {
        "32":"ico.png",
        "48":"ico.png",
        "128":"ico.png"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "ico.png"
    },
    "permissions": [
        "tabs"
      ]
  }